# DATABASE

POSTGRES_USER=prisma
POSTGRES_PASSWORD=prisma
POSTGRES_DB=paladindeck
DATABASE_URL="postgresql://prisma:prisma@postgres:5432/paladindeck?paladindeck=public&sslmode=prefer&pool_timeout=30"

# SCRYFALL API

SCRYFALL_API_URI=https://api.scryfall.com

# REDIS

REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=password

# Downloads

DOWNLOADS_DIR=/tmp/downloads
BULK_DATA_FILE_NAME=bulk_data
